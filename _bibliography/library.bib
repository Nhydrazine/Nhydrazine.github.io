	journal = {PLoS Pathog},
	journal-full = {PLoS pathogens},
	mesh = {Animals; Humans; Antibodies, Viral; Virus Diseases; Influenza, Human; Receptors, Fc; Antiviral Agents; Antibodies, Neutralizing; HIV Antibodies},
