	pmid = {35544074},
	pst = {ppublish},
	title = {Effect of 3BNC117 and romidepsin on the HIV-1 reservoir in people taking suppressive antiretroviral therapy (ROADMAP): a randomised, open-label, phase 2A trial},
	volume = {3},
