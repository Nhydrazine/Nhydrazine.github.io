	keywords = {Bispecific antibody; Developability; Fc engineering; Heterodimeric Fc; Tumor-associated antigen × CD3},
	mesh = {Antibodies, Bispecific; Antibodies, Monoclonal; Antigens, Neoplasm; CD3 Complex; Humans; Protein Engineering},
